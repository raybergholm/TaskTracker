{
    "_version": "0.1.0",
    "sap.ui5": {
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "localisation/texts.properties"
            },
            "lang": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/languages.json"
            },
            "file": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": ""
            },
            "taskMetadata": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/taskMetadata.json"
            },
            "": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/mockData.json"
            }
        },
        "resources": {
            "js": [{
                    "uri": "plugins/moment.min.js"
                },
                {
                    "uri": "plugins/js-utils.js"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "com.tasky.view",
                "controlId": "Tasky--TaskyApp",
                "controlAggregation": "detailPages"
            },
            "routes": [{
                    "pattern": "",
                    "name": "default",
                    "target": "Tasks"
                },
                {
                    "pattern": "Tasks",
                    "name": "Tasks",
                    "target": "Tasks"
                },
                {
                    "pattern": "Overview",
                    "name": "Overview",
                    "target": "Overview"
                },
                {
                    "pattern": "UserSettings",
                    "name": "UserSettings",
                    "target": "UserSettings"
                }
            ],
            "targets": {
                "Tasks": {
                    "viewName": "TaskDetail",
                    "transition": "flip"
                },
                "Overview": {
                    "viewName": "StatusOverview",
                    "transition": "flip"
                },
                "UserSettings": {
                    "viewName": "UserSettings",
                    "transition": "flip"
                }
            }
        }
    }
}
