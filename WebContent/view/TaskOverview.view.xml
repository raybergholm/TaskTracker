<core:View
	controllerName="com.tasky.controller.TaskOverview"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc">
    <SplitContainer id="taskDisplaySplitContainer"
        initialMaster="MyTaskList"
        initialDetail="TaskDetail">
        <masterPages>
            <mvc:XMLView id="MyTaskList" viewName="com.tasky.view.MyTaskList" />
        </masterPages>
        <detailPages>
            <mvc:XMLView id="TaskDetail" viewName="com.tasky.view.TaskDetail" />
        </detailPages>
    </SplitContainer>
</core:View>
